<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta property="og:image" content="~/ResourcePackages/gds_template/assets/dist/images/opengraph-image.png">
    <title>@ViewData["Title"] - Careers.Freshlook</title>

    <link rel="stylesheet" href="~/css/govuk-template.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/fonts.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/govuk.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/main.min.css" asp-append-version="true" />
    <link rel="shortcut icon" href="~/images/favicon.ico" type="image/x-icon" />
    <link rel="mask-icon" href="~/images/gov.uk_logotype_crown.svg" color="#0b0c0c">
    <link rel="apple-touch-icon-precomposed" sizes="152x152" href="~/images/apple-touch-icon-152x152.png">
    <link rel="apple-touch-icon-precomposed" sizes="120x120" href="~/images/apple-touch-icon-120x120.png">
    <link rel="apple-touch-icon-precomposed" sizes="76x76" href="~/images/apple-touch-icon-76x76.png">
    <link rel="apple-touch-icon-precomposed" href="~/images/apple-touch-icon-60x60.png">
</head>
<body>
    <script type="text/javascript">document.body.className = ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');</script>
    <div id="global-cookie-message">
        <p>Find a career uses cookies to make the site simpler. <a href="/help/cookies">Find out more about cookies</a></p>
    </div>
    <header class="with-proposition" id="global-header" role="banner">
        <div class="header-wrapper">
            <div class="header-proposition">
                <div class="content">
                    <nav id="proposition-menu"><a href="/" id="proposition-name">Find a career</a><strong class="phase-tag">SPIKE</strong>
                    </nav>
                </div>
            </div>
        </div>
    </header>
    <main role="main">
        <div class="content-container">
            @RenderSection("BannerContainer", required: false)
        </div>
        <div id="PublicWrapper">
            @RenderSection("BreadcrumbExtender", required: false)
            @RenderSection("MainContentTop", required: false)
            @RenderBody()
            @RenderSection("MainContentBottom", required: false)
        </div>
    </main>
    <footer class="group js-footer" id="footer" role="contentinfo">
        <div class="footer-wrapper">
            <div class="footer-meta">
                <div class="footer-meta-inner">
                    <ul>
                        <li><a href="/help/cookies">Privacy and cookies</a></li>
                        <li><a href="/help/terms-and-conditions">Terms and conditions</a></li>
                    </ul>
                    <div class="open-government-licence">
                        <p class="logo"><a href="https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/" rel="license">Open Government Licence</a></p>
                        <p>All content is available under the <a href="https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/" rel="license">Open Government Licence v3.0</a>, except where otherwise stated</p>
                    </div>
                </div>
                <div class="copyright"><a href="http://www.nationalarchives.gov.uk/information-management/re-using-public-sector-information/copyright-and-re-use/crown-copyright/">&copy; Crown copyright</a>
                </div>
            </div>
        </div>
    </footer>
    <script src="~/js/jquerybundle.min.js" asp-append-version="true"></script>
    <script src="~/js/govuksel.min.js" asp-append-version="true"></script>
    <environment include="Development">
        <script src="~/src/js/dfcdigital.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/dfcdigital.min.js" asp-append-version="true"></script>
    </environment>
    @RenderSection("Scripts", required: false)
</body>
</html>
